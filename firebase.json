{
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "headers": [
    {
      "source": "**/*.@(webp)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/webp"
        }
      ]
    }
  ]
}
